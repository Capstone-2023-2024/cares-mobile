    "rmdircmd": "rmdir /s /q node_modules",
    "prune": "git remote prune origin",
    "android": "react-native run-android",
    "ios": "react-native run-ios",
    "lint": "cls && eslint . --fix && prettier . --write --tailwind-config \"./tailwind.config.js\"",
    "test": "jest",
    "prepare": "husky install",
    "emulator": "emulator -avd Pixel_4_API_33 -netdelay none -netspeed full",
    "upgrade": "react-native upgrade"
  },
  "dependencies": {
    "@react-native-async-storage/async-storage": "^1.19.2",
